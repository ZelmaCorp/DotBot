# Staging Environment Configuration
# Copy this file to .env on your staging server

# =============================================================================
# CORE CONFIGURATION
# =============================================================================

NODE_ENV=staging
DEBUG=false
# LOG_LEVEL: Use lowercase format (info, warn, error, etc.)
LOG_LEVEL=info

# =============================================================================
# DOMAIN CONFIGURATION
# =============================================================================

DOMAIN_NAME=sandbox.dotbot.zelmacorp.io
ACME_EMAIL=admin@zelmacorp.io

# =============================================================================
# APPLICATION URLs
# =============================================================================
# NOTE: Base URLs should NOT include /api suffix
# API paths will be appended by application code

FRONTEND_URL=https://sandbox.dotbot.zelmacorp.io
BACKEND_URL=https://sandbox.dotbot.zelmacorp.io
WS_URL=wss://sandbox.dotbot.zelmacorp.io

# =============================================================================
# AI PROVIDER CONFIGURATION
# =============================================================================

AI_PROVIDER=asi-one
ASI_ONE_API_KEY=your_asi_one_api_key_here
ASI_ONE_ENDPOINT=https://agentverse.ai/api/v1
ASI_ONE_BASE_URL=https://api.asi1.ai/v1

# =============================================================================
# CORS CONFIGURATION
# =============================================================================

CORS_ORIGINS=*
